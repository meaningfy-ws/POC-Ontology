rmo:essentialFor rdfs:domain rmo:Skill ;
    rdfs:range rmo:Role .

rmo:hasBenefits rdfs:domain rmo:TypeOfEngagement ;
    rdfs:range rmo:Benefits .

rmo:hasCredential rdfs:domain rmo:CandidateProfile ;
    rdfs:range rmo:Credential .

rmo:hasExperience rdfs:domain rmo:CandidateProfile ;
    rdfs:range rmo:Experience .

rmo:hasProfile rdfs:domain rmo:Candidate ;
    rdfs:range rmo:CandidateProfile .

rmo:hasQualification rdfs:domain rmo:CandidateProfile ;
    rdfs:range rmo:Qualification .

rmo:hasSkillUsed rdfs:domain rmo:Experience ;
    rdfs:range rmo:Skill .

rmo:isRelatedTo rdf:domain rmo:Experience ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( rmo:Role rmo:Qualification ) ] .

rmo:optionalFor rdfs:domain rmo:Skill ;
    rdfs:range rmo:Role .

rmo:posts rdfs:domain rmo:Company ;
    rdfs:range rmo:JobDescription .

rmo:receivesJobDesciption rdfs:domain rmo:CandidateProfile ;
    rdfs:range rmo:JobDescription .

rmo:requiresCredential rdfs:domain rmo:JobDescription ;
    rdfs:range rmo:Credential .

rmo:requiresExperience rdfs:domain rmo:JobDescription ;
    rdfs:range rmo:Experience .

rmo:requiresQualification rdfs:domain rmo:JobDescription ;
    rdfs:range rmo:Qualification .

rmo:specifiesLocation rdfs:domain rmo:JobDescription ;
    rdfs:range dcterms:Location .

rmo:targetOccupation rdfs:domain rmo:CandidateProfile ;
    rdfs:range rmo:Role .

esco:Occupation a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue esco:Occupation ;
            owl:onProperty skos:inScheme ] .

esco:Qualification a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue esco:Qualification ;
            owl:onProperty skos:inScheme ] .

esco:Skill a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue esco:Skill ;
            owl:onProperty skos:inScheme ] .

rmo:BenefitName a owl:FunctionalProperty ;
    rdfs:domain rmo:Benefits ;
    rdfs:range rdf:PlainLiteral .

rmo:availability a owl:FunctionalProperty ;
    rdfs:domain rmo:JobDescription ;
    rdfs:range rdf:PlainLiteral .

rmo:companyName a owl:FunctionalProperty ;
    rdfs:domain rmo:Company ;
    rdfs:range rdf:PlainLiteral .

rmo:dateAwarded a owl:FunctionalProperty ;
    rdfs:domain rmo:Credential ;
    rdfs:range xsd:date .

rmo:educationDegree a owl:FunctionalProperty ;
    rdfs:domain rmo:Education ;
    rdfs:range rdf:PlainLiteral .

rmo:engagementType a owl:FunctionalProperty ;
    rdfs:domain rmo:TypeOfEngagement ;
    rdfs:range rdf:PlainLiteral .

rmo:hasEngagement a owl:FunctionalProperty ;
    rdfs:domain rmo:JobDescription ;
    rdfs:range rmo:TypeOfEngagement .

rmo:hasSkills rdfs:domain rmo:CandidateProfile ;
    rdfs:range rmo:Skill .

rmo:industry a owl:FunctionalProperty ;
    rdfs:domain rmo:Company ;
    rdfs:range rdf:PlainLiteral .

rmo:jobId a owl:FunctionalProperty ;
    rdfs:domain rmo:JobDescription ;
    rdfs:range rdf:PlainLiteral .

rmo:offersPosition a owl:FunctionalProperty ;
    rdfs:domain rmo:JobDescription ;
    rdfs:range rmo:Role .

rmo:phoneNumber a owl:FunctionalProperty ;
    rdfs:domain rmo:Candidate ;
    rdfs:range rdf:PlainLiteral .

rmo:profileId a owl:FunctionalProperty ;
    rdfs:domain rmo:CandidateProfile ;
    rdfs:range xsd:integer .

rmo:projectType a owl:FunctionalProperty ;
    rdfs:domain rmo:JobDescription ;
    rdfs:range rdf:PlainLiteral .

rmo:rate a owl:FunctionalProperty ;
    rdfs:domain rmo:JobDescription ;
    rdfs:range xsd:decimal .

rmo:requiresSkill rdfs:domain rmo:JobDescription ;
    rdfs:range rmo:Skill .

rmo:startDate a owl:FunctionalProperty ;
    rdfs:domain rmo:JobDescription ;
    rdfs:range xsd:Date .

rmo:targetRate a owl:FunctionalProperty ;
    rdfs:domain rmo:CandidateProfile ;
    rdfs:range xsd:double .

rmo:timeZone a owl:FunctionalProperty ;
    rdfs:domain dcterms:Location ;
    rdfs:range rdf:PlainLiteral .

rmo:yearsOfExperience a owl:FunctionalProperty ;
    rdfs:domain rmo:Experience ;
    rdfs:range xsd:Integer .

rmo:yearsOfSkill a owl:FunctionalProperty ;
    rdfs:domain rmo:Skill ;
    rdfs:range xsd:integer .

rdf:Plainliteral a owl:Class .

locn:geographicName a owl:FunctionalProperty ;
    rdfs:domain dcterms:Location ;
    rdfs:range rdf:PlainLiteral .

foaf:homepage a owl:FunctionalProperty ;
    rdfs:domain rmo:Candidate ;
    rdfs:range xsd:anyURI .

foaf:mbox a owl:FunctionalProperty ;
    rdfs:domain rmo:Candidate ;
    rdfs:range rdf:PlainLiteral .

foaf:name a owl:FunctionalProperty ;
    rdfs:domain rmo:Candidate ;
    rdfs:range rdf:PlainLiteral .

rmo:Certification a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:description ] .

rmo:Education rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:educationDegree ] .

rmo:hasLocation a owl:FunctionalProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( rmo:Company rmo:CandidateProfile ) ] ;
    rdfs:range dcterms:Location .

rdf:plainLiteral a owl:Class .

rmo:Benefits a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:BenefitName ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:description ] .

skos:inScheme rdfs:domain [ a owl:Class ;
            owl:unionOf ( rmo:Qualification rmo:Role rmo:Skill ) ] ;
    rdfs:range skos:Concept .

skos:prefLabel a owl:FunctionalProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( rmo:Qualification rmo:Role rmo:Skill ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( rdf:plainLiteral rdf:PlainLiteral ) ] .

rmo:Credential a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:dateAwarded ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:title ] .

rmo:TypeOfEngagement a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:engagementType ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty skos:altLabel ] .

dcterms:Location rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:timeZone ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty locn:geographicName ] .

skos:altLabel a owl:FunctionalProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( rmo:Qualification rmo:Role rmo:Skill rmo:TypeOfEngagement ) ] ;
    rdfs:range rdf:PlainLiteral .

rmo:Company a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:companyName ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:description ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:hasLocation ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:industry ] .

rmo:Candidate a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty foaf:homepage ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:title ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:phoneNumber ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty foaf:mbox ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty foaf:name ] .

dcterms:description a owl:FunctionalProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( rmo:Benefits rmo:Certification rmo:Company rmo:Experience rmo:JobDescription rmo:Qualification ) ] ;
    rdfs:range rdf:PlainLiteral .

rmo:Experience a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:yearsOfExperience ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:title ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:description ] .

dcterms:title a owl:FunctionalProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( rmo:Candidate rmo:Credential rmo:Experience rmo:JobDescription rmo:Qualification rmo:Role rmo:Skill ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( rdf:PlainLiteral rdf:Plainliteral rdf:plainLiteral ) ] .

rmo:Qualification a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty skos:prefLabel ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty skos:altLabel ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:description ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:title ] .

rmo:CandidateProfile rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:profileId ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:hasLocation ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty rmo:hasSkills ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:targetRate ] .

rmo:Role a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:title ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty skos:prefLabel ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty skos:altLabel ] .

rmo:Skill a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:title ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty skos:prefLabel ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:yearsOfSkill ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty skos:altLabel ] .

rmo:JobDescription a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:jobId ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:projectType ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:title ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:offersPosition ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:startDate ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:availability ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:rate ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty rmo:requiresSkill ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:description ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rmo:hasEngagement ] .

rdf:PlainLiteral a owl:Class .

[] a owl:AllDisjointClasses ;
    owl:members ( rmo:Certification rmo:Education ) .

[] a owl:AllDisjointClasses ;
    owl:members ( rmo:Qualification rmo:Role rmo:Skill ) .

