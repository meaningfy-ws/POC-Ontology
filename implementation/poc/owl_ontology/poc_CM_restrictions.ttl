    rdfs:range rdf:PlainLiteral .

:jobId a owl:FunctionalProperty ;
    rdfs:domain :JobDescription ;
    rdfs:range rdf:PlainLiteral .

:offersPosition a owl:FunctionalProperty ;
    rdfs:domain :JobDescription ;
