    rdfs:range rdf:PlainLiteral .

:yearsOfExperience a owl:FunctionalProperty ;
    rdfs:domain :Experience ;
    rdfs:range xsd:Integer .

:yearsOfSkill a owl:FunctionalProperty ;
    rdfs:domain :Skill ;
    rdfs:range xsd:integer .

rdf:Plainliteral a owl:Class .

locn:geographicName a owl:FunctionalProperty ;
    rdfs:domain dcterms:Location ;
    rdfs:range rdf:PlainLiteral .

foaf:homepage a owl:FunctionalProperty ;
    rdfs:domain :Candidate ;
    rdfs:range xsd:anyURI .

foaf:mbox a owl:FunctionalProperty ;
    rdfs:domain :Candidate ;
    rdfs:range rdf:PlainLiteral .

foaf:name a owl:FunctionalProperty ;
    rdfs:domain :Candidate ;
    rdfs:range rdf:PlainLiteral .

:Certification a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:description ] .

:Education rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :educationDegree ] .

:hasLocation a owl:FunctionalProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :Company :CandidateProfile ) ] ;
    rdfs:range dcterms:Location .

rdf:plainLiteral a owl:Class .

:Benefits a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :BenefitName ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:description ] .

skos:inScheme rdfs:domain [ a owl:Class ;
            owl:unionOf ( :Qualification :Role :Skill ) ] ;
    rdfs:range skos:Concept .

skos:prefLabel a owl:FunctionalProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :Qualification :Role :Skill ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( rdf:plainLiteral rdf:PlainLiteral ) ] .

:Credential a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :dateAwarded ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:title ] .

:TypeOfEngagement a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :engagementType ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty skos:altLabel ] .

dcterms:Location rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :timeZone ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty locn:geographicName ] .

skos:altLabel a owl:FunctionalProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :Qualification :Role :Skill :TypeOfEngagement ) ] ;
    rdfs:range rdf:PlainLiteral .

:Company a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :hasLocation ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :industry ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:description ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :companyName ] .

:Candidate a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty foaf:name ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty foaf:mbox ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :phoneNumber ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:title ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty foaf:homepage ] .

dcterms:description a owl:FunctionalProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :Benefits :Certification :Company :Experience :JobDescription :Qualification ) ] ;
    rdfs:range rdf:PlainLiteral .

:Experience a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:title ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:description ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :yearsOfExperience ] .

dcterms:title a owl:FunctionalProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :Candidate :Credential :Experience :JobDescription :Qualification :Role :Skill ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( rdf:PlainLiteral rdf:Plainliteral rdf:plainLiteral ) ] .

:Qualification a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty skos:prefLabel ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty skos:altLabel ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:title ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:description ] .

:CandidateProfile rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :hasLocation ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty :hasSkills ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :profileId ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :targetRate ] .

:Role a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
