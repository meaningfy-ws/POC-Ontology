        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty skos:altLabel ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:title ] .

:Skill a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty skos:prefLabel ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty skos:altLabel ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:title ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :yearsOfSkill ] .

:JobDescription a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :projectType ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:title ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :offersPosition ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :availability ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :startDate ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :jobId ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:description ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :rate ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :hasEngagement ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty :requiresSkill ] .

rdf:PlainLiteral a owl:Class .

[] a owl:AllDisjointClasses ;
    owl:members ( :Certification :Education ) .

[] a owl:AllDisjointClasses ;
    owl:members ( :Qualification :Role :Skill ) .

