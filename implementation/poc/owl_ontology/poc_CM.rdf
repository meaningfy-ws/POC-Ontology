   <owl:ObjectProperty rdf:about="http://index.dev/ontology/poc#specifiesLocation"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#specifiesLocation">
      <skos:prefLabel xml:lang="en">Specifies location</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#specifiesLocation">
      <skos:definition xml:lang="en">Specifies the geographical location and timezone relevant to JobDescription. (poc:JobDescription -&gt; dct:Location (+poc:specifiesLocation))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://index.dev/ontology/poc#hasLocation"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#hasLocation">
      <skos:prefLabel xml:lang="en">Has location</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#hasLocation">
      <skos:definition xml:lang="en">Specifies the geographical location and timezone relevant to CandidateProfile. (poc:Company -&gt; dct:Location (+poc:hasLocation)) Specifies the geographical location and timezone relevant to CandidateProfile. (poc:CandidateProfile -&gt; dct:Location (+poc:hasLocation))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://index.dev/ontology/poc#hasBenefits"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#hasBenefits">
      <skos:prefLabel xml:lang="en">Has benefits</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#hasBenefits">
      <skos:definition xml:lang="en">Indicates the benefits associated with a specific type of job engagement. (poc:TypeOfEngagement -&gt; poc:Benefits (+poc:hasBenefits))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://index.dev/ontology/poc#hasProfile"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#hasProfile">
      <skos:prefLabel xml:lang="en">Has profile</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#hasProfile">
      <skos:definition xml:lang="en">Indicates the CandidateProfile of a specific Candidate. (poc:Candidate -&gt; poc:CandidateProfile (+poc:hasProfile))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://index.dev/ontology/poc#hasExperience"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#hasExperience">
      <skos:prefLabel xml:lang="en">Has experience</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#hasExperience">
      <skos:definition xml:lang="en">Represents the work experience details of a CandidateProfile. (poc:CandidateProfile -&gt; poc:Experience (+poc:hasExperience))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://index.dev/ontology/poc#receivesJobDesciption"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#receivesJobDesciption">
      <skos:prefLabel xml:lang="en">Receives job desciption</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#receivesJobDesciption">
      <skos:definition xml:lang="en">Indicates that a CandidateProfile receives a JobDescription from the system. (poc:CandidateProfile -&gt; poc:JobDescription (+poc:receivesJobDesciption))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://index.dev/ontology/poc#targetOccupation"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#targetOccupation">
      <skos:prefLabel xml:lang="en">Target occupation</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#targetOccupation">
      <skos:definition xml:lang="en">Indicates the job roles or occupations that a CandidateProfile is interested in or qualified for. (poc:CandidateProfile -&gt; poc:Role (+poc:targetOccupation))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://index.dev/ontology/poc#hasQualification"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#hasQualification">
      <skos:prefLabel xml:lang="en">Has qualification</skos:prefLabel>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://index.dev/ontology/poc#hasCredential"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#hasCredential">
      <skos:prefLabel xml:lang="en">Has credential</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#hasCredential">
      <skos:definition xml:lang="en">Academic qualifications and professional certifications obtained by a candidate. (poc:CandidateProfile -&gt; poc:Credential (+poc:hasCredential))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://index.dev/ontology/poc#hasSkills"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#hasSkills">
      <skos:prefLabel xml:lang="en">Has skills</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#hasSkills">
      <skos:definition xml:lang="en">Indicates that a CandidateProfile possesses a specific skill. (poc:CandidateProfile -&gt; poc:Skill (+poc:hasSkills))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://index.dev/ontology/poc#posts"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#posts">
      <skos:prefLabel xml:lang="en">Posts</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#posts">
      <skos:definition xml:lang="en">Indicates that a Company publishes a JobDescription. (poc:Company -&gt; poc:JobDescription (+poc:posts))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://index.dev/ontology/poc#requiresCredential"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#requiresCredential">
      <skos:prefLabel xml:lang="en">Requires credential</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#requiresCredential">
      <skos:definition xml:lang="en">Indicates that a JobDescription may require one or more Academic qualifications and/or professional certifications. (poc:JobDescription -&gt; poc:Credential (+poc:requiresCredential))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://index.dev/ontology/poc#isRelatedTo"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#isRelatedTo">
      <skos:prefLabel xml:lang="en">Is related to</skos:prefLabel>
