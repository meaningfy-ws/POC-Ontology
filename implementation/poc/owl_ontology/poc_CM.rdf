   <owl:Class rdf:about="http://purl.org/dc/terms/Location"/>
   <rdf:Description rdf:about="http://purl.org/dc/terms/Location">
      <skos:prefLabel xml:lang="en">Location</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://purl.org/dc/terms/Location">
      <skos:definition xml:lang="en">A geographic area or place characterized by its physical coordinates and corresponding time zone.</skos:definition>
   </rdf:Description>
   <owl:Class rdf:about="http://index.dev/ontology/poc#Benefits"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Benefits">
      <skos:prefLabel xml:lang="en">Benefits</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Benefits">
      <skos:definition xml:lang="en">Additional advantages provided to employees beyond their salary, such as health insurance or retirement plans.</skos:definition>
   </rdf:Description>
   <owl:Class rdf:about="http://index.dev/ontology/poc#Candidate"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Candidate">
      <skos:prefLabel xml:lang="en">Candidate</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Candidate">
      <skos:definition xml:lang="en">An individual who uses the system to find job opportunities.</skos:definition>
   </rdf:Description>
   <owl:Class rdf:about="http://index.dev/ontology/poc#CandidateProfile"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#CandidateProfile">
      <skos:prefLabel xml:lang="en">Candidate profile</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#CandidateProfile">
      <skos:definition xml:lang="en">Represents the detailed profile of a Candidate, including credentials, skills experience, and location.</skos:definition>
   </rdf:Description>
   <owl:Class rdf:about="http://index.dev/ontology/poc#Certification"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Certification">
      <skos:prefLabel xml:lang="en">Certification</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Certification">
      <skos:definition xml:lang="en">The official recognition of a candidate's expertise or proficiency in a specific domain, typically obtained through examination or evaluation by a professional body.</skos:definition>
   </rdf:Description>
   <owl:Class rdf:about="http://index.dev/ontology/poc#Company"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Company">
      <skos:prefLabel xml:lang="en">Company</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Company">
      <skos:definition xml:lang="en">An organization or business entity that employs individuals.</skos:definition>
   </rdf:Description>
   <owl:Class rdf:about="http://index.dev/ontology/poc#Contract"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Contract">
      <skos:prefLabel xml:lang="en">Contract</skos:prefLabel>
   </rdf:Description>
   <owl:Class rdf:about="http://index.dev/ontology/poc#Credential"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Credential">
      <skos:prefLabel xml:lang="en">Credential</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Credential">
      <skos:definition xml:lang="en">An academic or professional qualification obtained by a candidate, such as degrees or certifications.</skos:definition>
   </rdf:Description>
   <owl:Class rdf:about="http://index.dev/ontology/poc#Education"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Education">
      <skos:prefLabel xml:lang="en">Education</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Education">
      <skos:definition xml:lang="en">The academic qualifications of a Candidate.</skos:definition>
   </rdf:Description>
   <owl:Class rdf:about="http://index.dev/ontology/poc#Experience"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Experience">
      <skos:prefLabel xml:lang="en">Experience</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Experience">
      <skos:definition xml:lang="en">Represents the practical knowledge and proficiency gained by a candidate through previous work engagements, projects, or hands-on activities.</skos:definition>
   </rdf:Description>
   <owl:Class rdf:about="http://index.dev/ontology/poc#JobDescription"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#JobDescription">
      <skos:prefLabel xml:lang="en">Job description</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#JobDescription">
      <skos:definition xml:lang="en">A job opening posted by a company, including job details and requirements.</skos:definition>
   </rdf:Description>
   <owl:Class rdf:about="http://index.dev/ontology/poc#Qualification"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Qualification">
      <skos:prefLabel xml:lang="en">Qualification</skos:prefLabel>
   </rdf:Description>
   <owl:Class rdf:about="http://index.dev/ontology/poc#Qualification"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Qualification">
      <skos:prefLabel xml:lang="en">Qualification</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Qualification">
      <skos:definition xml:lang="en">Qualifications are the formal outcome of an assessment and validation process by a competent body determining that an individual has achieved learning outcomes to given standards.</skos:definition>
   </rdf:Description>
   <owl:Class rdf:about="http://index.dev/ontology/poc#Role"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Role">
      <skos:prefLabel xml:lang="en">Role</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Role">
      <skos:definition xml:lang="en">Represents the specific job occupations, positions, or professions for which candidates may be qualified or seeking employment opportunities.</skos:definition>
   </rdf:Description>
   <owl:Class rdf:about="http://index.dev/ontology/poc#Skill"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Skill">
      <skos:prefLabel xml:lang="en">Skill</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#Skill">
      <skos:definition xml:lang="en">A specific capability or proficiency possessed by a candidate, often acquired through training, practice, or experience.</skos:definition>
   </rdf:Description>
   <owl:Class rdf:about="http://index.dev/ontology/poc#TypeOfEngagement"/>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#TypeOfEngagement">
      <skos:prefLabel xml:lang="en">Type of engagement</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://index.dev/ontology/poc#TypeOfEngagement">
      <skos:definition xml:lang="en">Represents various arrangements under which individuals are employed, such as part-time, full-time, or contractual roles.</skos:definition>
   </rdf:Description>
