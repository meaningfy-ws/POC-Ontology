    sh:name "Has profile" ;
    sh:path poc:hasProfile ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://index.dev/ontology/poc#hasProfile> ?that . ?that <http://index.dev/ontology/poc#hasProfile> ?this .}" ] .

