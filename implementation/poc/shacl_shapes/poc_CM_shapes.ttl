tech-shape:dct-Location a sh:NodeShape ;
    rdfs:label "Location" ;
    rdfs:comment "A geographic area or place characterized by its physical coordinates and corresponding time zone." ;
    rdfs:isDefinedBy tech-shape:tech-shape ;
    sh:property tech-shape:dct-Location-locn-geographicName,
        tech-shape:dct-Location-poc-timeZone ;
    sh:targetClass dcterms:Location .

tech-shape:poc-Benefits a sh:NodeShape ;
    rdfs:label "Benefits" ;
    rdfs:comment "Additional advantages provided to employees beyond their salary, such as health insurance or retirement plans." ;
    rdfs:isDefinedBy tech-shape:tech-shape ;
    sh:property tech-shape:poc-Benefits-dct-description,
        tech-shape:poc-Benefits-poc-BenefitName ;
    sh:targetClass poc:Benefits .

tech-shape:poc-Candidate a sh:NodeShape ;
    rdfs:label "Candidate" ;
    rdfs:comment "An individual who uses the system to find job opportunities." ;
    rdfs:isDefinedBy tech-shape:tech-shape ;
    sh:property tech-shape:poc-Candidate-dct-title,
        tech-shape:poc-Candidate-foaf-homepage,
        tech-shape:poc-Candidate-foaf-mbox,
        tech-shape:poc-Candidate-foaf-name,
        tech-shape:poc-Candidate-poc-hasProfile,
        tech-shape:poc-Candidate-poc-phoneNumber ;
    sh:targetClass poc:Candidate .

tech-shape:poc-CandidateProfile a sh:NodeShape ;
    rdfs:label "Candidate profile" ;
    rdfs:comment "Represents the detailed profile of a Candidate, including credentials, skills experience, and location." ;
    rdfs:isDefinedBy tech-shape:tech-shape ;
    sh:property tech-shape:poc-CandidateProfile-poc-hasCredential,
        tech-shape:poc-CandidateProfile-poc-hasExperience,
        tech-shape:poc-CandidateProfile-poc-hasLocation,
        tech-shape:poc-CandidateProfile-poc-hasQualification,
        tech-shape:poc-CandidateProfile-poc-hasSkills,
        tech-shape:poc-CandidateProfile-poc-profileId,
        tech-shape:poc-CandidateProfile-poc-receivesJobDesciption,
        tech-shape:poc-CandidateProfile-poc-targetOccupation,
        tech-shape:poc-CandidateProfile-poc-targetRate ;
    sh:targetClass poc:CandidateProfile .

tech-shape:poc-Certification a sh:NodeShape ;
    rdfs:label "Certification" ;
    rdfs:comment "The official recognition of a candidate's expertise or proficiency in a specific domain, typically obtained through examination or evaluation by a professional body." ;
    rdfs:isDefinedBy tech-shape:tech-shape ;
    sh:property tech-shape:poc-Certification-dct-description ;
    sh:targetClass poc:Certification .

tech-shape:poc-Company a sh:NodeShape ;
    rdfs:label "Company" ;
    rdfs:comment "An organization or business entity that employs individuals." ;
    rdfs:isDefinedBy tech-shape:tech-shape ;
    sh:property tech-shape:poc-Company-dct-description,
        tech-shape:poc-Company-poc-companyName,
        tech-shape:poc-Company-poc-hasLocation,
        tech-shape:poc-Company-poc-industry,
        tech-shape:poc-Company-poc-posts ;
    sh:targetClass poc:Company .

tech-shape:poc-Contract a sh:NodeShape ;
    rdfs:label "Contract" ;
    rdfs:isDefinedBy tech-shape:tech-shape ;
    sh:targetClass poc:Contract .

tech-shape:poc-Credential a sh:NodeShape ;
    rdfs:label "Credential" ;
    rdfs:comment "An academic or professional qualification obtained by a candidate, such as degrees or certifications." ;
    rdfs:isDefinedBy tech-shape:tech-shape ;
    sh:property tech-shape:poc-Credential-dct-title,
        tech-shape:poc-Credential-poc-dateAwarded ;
    sh:targetClass poc:Credential .

tech-shape:poc-Education a sh:NodeShape ;
    rdfs:label "Education" ;
    rdfs:comment "The academic qualifications of a Candidate." ;
    rdfs:isDefinedBy tech-shape:tech-shape ;
    sh:property tech-shape:poc-Education-poc-educationDegree ;
    sh:targetClass poc:Education .

tech-shape:poc-Experience a sh:NodeShape ;
    rdfs:label "Experience" ;
    rdfs:comment "Represents the practical knowledge and proficiency gained by a candidate through previous work engagements, projects, or hands-on activities." ;
    rdfs:isDefinedBy tech-shape:tech-shape ;
    sh:property tech-shape:poc-Experience-dct-description,
        tech-shape:poc-Experience-dct-title,
        tech-shape:poc-Experience-poc-hasSkillUsed,
        tech-shape:poc-Experience-poc-isRelatedTo,
        tech-shape:poc-Experience-poc-yearsOfExperience ;
    sh:targetClass poc:Experience .

tech-shape:poc-JobDescription a sh:NodeShape ;
    rdfs:label "Job description" ;
    rdfs:comment "A job opening posted by a company, including job details and requirements." ;
    rdfs:isDefinedBy tech-shape:tech-shape ;
    sh:property tech-shape:poc-JobDescription-dct-description,
        tech-shape:poc-JobDescription-dct-title,
        tech-shape:poc-JobDescription-poc-availability,
        tech-shape:poc-JobDescription-poc-hasEngagement,
        tech-shape:poc-JobDescription-poc-jobId,
        tech-shape:poc-JobDescription-poc-offersPosition,
        tech-shape:poc-JobDescription-poc-projectType,
        tech-shape:poc-JobDescription-poc-rate,
        tech-shape:poc-JobDescription-poc-requiresCredential,
        tech-shape:poc-JobDescription-poc-requiresExperience,
        tech-shape:poc-JobDescription-poc-requiresQualification,
        tech-shape:poc-JobDescription-poc-requiresSkill,
        tech-shape:poc-JobDescription-poc-specifiesLocation,
        tech-shape:poc-JobDescription-poc-startDate ;
    sh:targetClass poc:JobDescription .

tech-shape:poc-Qualification a sh:NodeShape ;
    rdfs:label "Qualification" ;
    rdfs:comment "Qualifications are the formal outcome of an assessment and validation process by a competent body determining that an individual has achieved learning outcomes to given standards." ;
    rdfs:isDefinedBy tech-shape:tech-shape ;
    sh:property tech-shape:poc-Qualification-dct-description,
        tech-shape:poc-Qualification-dct-title,
        tech-shape:poc-Qualification-skos-altLabel,
        tech-shape:poc-Qualification-skos-inScheme,
        tech-shape:poc-Qualification-skos-prefLabel ;
    sh:targetClass poc:Qualification .

tech-shape:poc-Role a sh:NodeShape ;
    rdfs:label "Role" ;
    rdfs:comment "Represents the specific job occupations, positions, or professions for which candidates may be qualified or seeking employment opportunities." ;
    rdfs:isDefinedBy tech-shape:tech-shape ;
    sh:property tech-shape:poc-Role-dct-title,
        tech-shape:poc-Role-skos-altLabel,
        tech-shape:poc-Role-skos-inScheme,
        tech-shape:poc-Role-skos-prefLabel ;
    sh:targetClass poc:Role .

tech-shape:poc-Skill a sh:NodeShape ;
    rdfs:label "Skill" ;
    rdfs:comment "A specific capability or proficiency possessed by a candidate, often acquired through training, practice, or experience." ;
    rdfs:isDefinedBy tech-shape:tech-shape ;
    sh:property tech-shape:poc-Skill-dct-title,
        tech-shape:poc-Skill-poc-essentialFor,
        tech-shape:poc-Skill-poc-optionalFor,
        tech-shape:poc-Skill-poc-yearsOfSkill,
        tech-shape:poc-Skill-skos-altLabel,
        tech-shape:poc-Skill-skos-inScheme,
        tech-shape:poc-Skill-skos-prefLabel ;
    sh:targetClass poc:Skill .

tech-shape:poc-TypeOfEngagement a sh:NodeShape ;
    rdfs:label "Type of engagement" ;
    rdfs:comment "Represents various arrangements under which individuals are employed, such as part-time, full-time, or contractual roles." ;
    rdfs:isDefinedBy tech-shape:tech-shape ;
    sh:property tech-shape:poc-TypeOfEngagement-poc-engagementType,
        tech-shape:poc-TypeOfEngagement-poc-hasBenefits,
        tech-shape:poc-TypeOfEngagement-skos-altLabel ;
    sh:targetClass poc:TypeOfEngagement .

tech-shape:dct-Location-locn-geographicName a sh:PropertyShape ;
